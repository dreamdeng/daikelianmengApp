<script type="text/javascript">
    var wx;
    apiready = function(){
      wx = api.require('wx');
    }
    $(".iphone").click(function() {
        openwin_header("register_new", "登录", "register_new.html", {}, true);
    })

    $(".wechat").click(function() {
        wx.auth({
            apiKey: ''
        }, function(ret, err) {
            if (ret.status) {

            } else {
                btToast("系统繁忙，请稍后再试...", 2000);
            }
        });
    })
</script>

