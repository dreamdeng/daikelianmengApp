/* 合伙人-成为合伙人 */
img {
    width: 100%;
}
a {
    text-decoration: none;
    color: #fc9752;
}
.partner_page {
    /*margin-top: 0.6rem;*/
    position: relative;
}

.table_frist {
    display: flex;
    display: -webkit-flex;
    height: 1.35rem;
    border-bottom: 1px solid #eaeaea;
}

.table_frist > li {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    font-size: 0.39rem;
    color: #333333;
    margin-left: 0.33rem;
}

.table_frist > li.f_active {
    font-size: 0.49rem;
    color: #fc9752;
}

.partner_tab .zt {
    background: #fc9752;
    display: none;
}

.partner_tab .f_active .zt {
    display: block;
}

.wra_frist {
    display: none;
}

.table_second {
    display: flex;
    display: -webkit-flex;
    justify-content: center;
}

.table_second > li {
    width: 50%;
    font-size: 0.39rem;
    border-bottom: 1px solid transparent;
    text-align: center;
    color: #999999;
    height: 1.33rem;
    line-height: 1.33rem;
}

.table_second > li.s_active {
    color: #fc9752;
    border-bottom: 1px solid #fc9752;
}

.content_frist > .f_active, .content_second > .s_active, .content_third > .t_active {
    display: block;
}

.wra_second {
    display: none;
    /*position: relative;*/
}

.fixed_btn {
    position: fixed;
    bottom: 1.6rem;
    left: 0;
    width: 100%;
}

.table_third {
    display: flex;
    display: -webkit-flex;
    justify-content: space-around;
    align-items: flex-end;
}

.table_third > li {
    width: 2.17rem;
    height: 0.71rem;
    background: #ffffff;
    font-size: 0.36rem;
    border-radius: 0.27rem;
    color: #323232;
    text-align: center;
    line-height: 0.71rem;
}

.wra_third {
    display: none;
}

.active_text {
    display: none;
}

.t_active .active_text {
    display: inline-block;
}

.table_third > .t_active {
    width: 3.55rem;
    height: 1.01rem;
    line-height: 1.01rem;
    background: #fc9752;
    color: #ffffff;
}

.foot_bar {
    border-top: 1px solid #d7d7d9;
    height: 1.23rem;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #ffffff;
}

.gm_btn {
    background: #fc9752;
    color: #ffffff;
    font-size: 0.36rem;
    height: 100%;
    width: 2.35rem;
    text-align: center;
    line-height: 1.23rem;
}

.cp_info {
    /*border-top: 1px solid #d7d7d9;*/
    margin-left: 0.1rem;
    display: flex;
}

.kc_price {
    font-size: 0.44rem;
    color: #323232;
    line-height: 1.23rem;
    margin-right: 0.23rem;
}

.xy_deal {
    display: flex;
    align-items: center;
    font-size: 0.33rem;
}

.xy_checked {
    width: 0.4rem;
    height: 0.4rem;
    border: 1px solid #b7b7b7;
    position: relative;
    margin-right: 0.1rem;
}

.deal_check {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.xy_checked > .icon-duigou4 {
    font-size: 0.4rem;
    color: #fc9752;
    font-weight: bold;
}

.yh_main {
    padding: 0.57rem 0.59rem 0;
}

.yh_top {
    display: flex;
    justify-content: space-between;
    margin-bottom: 0.47rem;
    align-items: center;
}

.yh_info {
    display: flex;
    align-items: center;
}

.yh_icon {
    width: 1.49rem;
    height: 1.49rem;
    border-radius: 50%;
    color: #ffffff;
    background: #dddddd;
    margin: 0 0.17rem;
}

.dklm {
}

.dk_title {
    font-size: 0.41rem;
    color: #323232;
    margin-bottom: 0.11rem;
}

.dk_kf {
    font-size: 0.33rem;
    color: #999999;
}

.weixin_icon {
    width: 0.77rem;
    height: 0.65rem;
    margin: 0 auto 0.11rem;
}

.xz_text {
    line-height: 2;
    font-size: 0.36rem;
    color: #333333;
}

.foot_fx {
    height: 1.53rem;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.3);
}

.fx_bar {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-around;
}

.fx_btn {
    width: 1.33rem;
    height: 1.33rem;
}

.fx_tc {
    display: none;
    width: 2.09rem;
    height: 2.09rem;
    background: rgba(0, 0, 0, 0.5);
    border-radius: 0.47rem;
    position: fixed;
    top: 45%;
    left: 50%;
    margin-left: -1.04rem;
}

.fx_icon {
    font-size: 1.12rem;
    color: #fff;
    text-align: center;
    margin: 0.27rem 0 0.13rem;
}

.fx_text {
    font-size: 0.31rem;
    text-align: center;
    color: #fff;
}

.gm_tc {
    width: 100%;
    height: 100%;
    position: fixed;
    background: rgba(0, 0, 0, 0.5);
    top: 0;
    left: 0;
}

.tc_img {
    width: 6.8rem;
    height: 6.83rem;
    position: absolute;
    top: 25%;
    left: 50%;
    margin-left: -3.4rem;
}

.close_tc {
    width: 0.53rem;
    height: 0.53rem;
    font-size: 0.5rem;
    position: absolute;
    top: 20%;
    background: #cacbce;
    color: #333;
    border-radius: 50%;
    font-weight: bold;
    right: 10%;
}

.flor {
    float: right;
}

.flof {
    float: left;
}
.no_click{

}