img {
    width: 100%;
}

select {
    outline: none;
    border: none;
    background: transparent;
}

html, body {
    min-height: 100vh;
    background: #f5f5f5;
}

a {
    text-decoration: none;
    color: #fc9752;
}

a:active {
    text-decoration: none;
}

.table_frist {
    display: flex;
    display: -webkit-flex;
    height: 1.35rem;
    border-bottom: 1px solid #eaeaea;
    background: #fff;
}

.table_frist > li {
    display: flex;
    display: -webkit-flex;
    align-items: center;
    font-size: 0.39rem;
    color: #333333;
    margin-right: 0.47rem;
    margin-left: 0.33rem;
}

.table_frist > li.f_active {
    font-size: 0.49rem;
    color: #fc9752;
}

.partner_tab .zt {
    background: #fc9752;
    display: none;
}

.partner_tab .f_active .zt {
    display: block;
}

.wra_frist {
    display: none;
}

.table_second {
    display: flex;
    display: -webkit-flex;
    justify-content: center;
    background: #fff;
}

.table_second > li {
    width: 50%;
    font-size: 0.39rem;
    border-bottom: 1px solid transparent;
    text-align: center;
    color: #999999;
    height: 1.33rem;
    line-height: 1.33rem;
}

.table_second > li.s_active {
    color: #fc9752;
    border-bottom: 1px solid #fc9752;
}

.content_frist > .f_active, .content_second > .s_active, .content_third > .t_active {
    display: block;
}

.wra_second {
    display: none;
    /*position: relative;*/
    margin-bottom: 0.33rem;
}

.fixed_btn {
    position: fixed;
    bottom: 1.6rem;
    left: 0;
    width: 100%;
}

.table_third {
    display: flex;
    display: -webkit-flex;
    justify-content: space-around;
    align-items: flex-end;
}

.table_third > li {
    width: 2.17rem;
    height: 0.71rem;
    background: #ffffff;
    font-size: 0.36rem;
    border-radius: 0.27rem;
    color: #323232;
    text-align: center;
    line-height: 0.71rem;
}

.wra_third {
    display: none;
}

.active_text {
    display: none;
}

.t_active .active_text {
    display: inline-block;
}

.table_third > .t_active {
